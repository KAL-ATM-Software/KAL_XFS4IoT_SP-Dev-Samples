    <RootNamespace>CashRecycler.CashRecyclerTemplate</RootNamespace>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="KAL.XFS4IoT.SP-Dev.Framework.CashAcceptor" Version="2.2.0" />
    <PackageReference Include="KAL.XFS4IoT.SP-Dev.Framework.CashDispenser" Version="2.2.0" />
  </ItemGroup>

  <ItemGroup>
      <None Update="App.config">
          <CopyToOutputDirectory>Always</CopyToOutputDirectory>
      </None>
  </ItemGroup>

</Project>
